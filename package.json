{
  "name": "tasknator",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx prisma generate && (npx prisma db push --skip-generate || true) && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npx prisma generate",
    "worker": "ts-node --project tsconfig.worker.json src/lib/jobs/worker.ts",
    "db:push": "npx prisma db push",
    "db:migrate": "npx prisma migrate dev",
    "db:seed": "npx prisma db seed"
  },
  "dependencies": {
