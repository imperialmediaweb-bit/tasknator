    "archiver": "^7.0.0",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "ioredis": "^5.4.0",
    "libsodium-wrappers": "^0.7.13",
    "lucide-react": "^0.468.0",
    "marked": "^14.0.0",
    "nanoid": "^5.0.0",
    "next": "^14.2.35",
    "next-auth": "^4.24.0",
    "nodemailer": "^6.9.0",
